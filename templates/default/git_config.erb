[alias]
     co = checkout
     ci = commit  
     st = status  
     br = branch  
     hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
     lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
     type = cat-file -t  
     dump = cat-file -p
